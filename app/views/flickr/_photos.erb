<div class="flickr">
	<h3>Currently viewing: <%= user_id %></h3>
	<ul>
		<% photos.each do |row| %>
			<li>
				<% row.each do |p| %>
					<% unless p.nil? %>
						<%= link_to(image_tag(FlickRaw.url_s(p), :class => 'flickr_photo', :title => p.title, :border => 0, :size => '150x150'), FlickRaw.url_photopage(p)) %>
					<% end %>
				<% end %>
			</li>
		<% end %>
	</ul>
</div>